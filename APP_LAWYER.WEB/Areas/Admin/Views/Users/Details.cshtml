@model IReadOnlyList<APP_LAWYER.DAL.Entities.User>

@{
    ViewBag.Title = "Пользователи";
    Layout = "_Layout";
}

<table class="table">
    <thead>
    <tr>
        <th>Имя пользователя</th>
        <th>Email</th>
        <th>Телефон</th>
        <th>Действия</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.Phone</td>
            <td>
                <a href="@Url.Action("Update", "Users", new { area = "Admin", id = user.Id })">Редактировать</a> |
                <a href="@Url.Action("Delete", "Users", new { area = "Admin", id = user.Id })">Удалить</a>
            </td>
        </tr>
    }
    </tbody>
</table>