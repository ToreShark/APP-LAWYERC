@model dynamic

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<p>
    <a href="@Url.Action("Create", "User")" class="btn btn-primary">Создать документ</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>Имя подкатегории</th>
        <th>Описание</th>
        <th>Действия</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var documents in Model)
    {
        <tr>
            <td>@documents.Name</td>
            <td>@documents.Description</td>
            <td>
                <div class="d-flex">
                    <a href="@Url.Action("Update", "User", new { id = documents.Id })" class="btn btn-primary mr-2">Редактировать</a>
                    <a asp-action="Delete" asp-route-id="@documents.Id" class="btn btn-danger">Удалить</a>
                </div>
            </td>
        </tr>
    }
    </tbody>
</table>